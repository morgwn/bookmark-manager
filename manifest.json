{
  "manifest_version": 3,
  "name": "GoldenTab",
  "version": "1.0",
  "description": "A modern bookmark manager",
  "permissions": [
    "bookmarks",
    "tabs",
    "favicon",
    "storage",
    "sidePanel"
  ],
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}
